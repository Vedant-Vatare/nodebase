{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"compilerOptions": {
		"target": "ES2023",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"lib": ["ES2023"],
		"types": ["node"],
		"outDir": "./dist",
		"rootDir": "./src",
		"sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }

}
